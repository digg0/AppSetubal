<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <!-- Cor de fundo do círculo -->
    <solid android:color="#FFFF" />

    <!-- Borda do círculo (opcional) -->
    <stroke
        android:width="2dp"
        android:color="#000000" />

    <!-- Padding interno (opcional) -->
    <padding
        android:left="8dp"
        android:top="8dp"
        android:right="8dp"
        android:bottom="8dp" />
</shape>
